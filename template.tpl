// Enter your template code here.
const log = require('logToConsole');
const injectScript = require('injectScript');
const setInWindow = require('setInWindow');
const copyFromWindow = require('copyFromWindow');
const settings = {
  apiKey: data.apiKey,
  startRecording: data.startRecording == '1',
  createNewSession: data.createNewSession == '1',
  
};

if (data.sessionInfo) {
  const sessionInfo = {};
  data.sessionInfo.forEach(info => {
    sessionInfo[info.name] = info.value;
  });
  settings.sessionInfo = sessionInfo;
}
if (data.userInfo) {
  const userInfo = {};
  data.userInfo.forEach(info => {
    userInfo[info.name] = info.value;
  });
  settings.userInfo = userInfo;
}
log('config:', settings);
setInWindow('SessionRewindConfig', settings);

injectScript("https://rec.sessionrewind.com/srloader.js", () => {
  log(copyFromWindow('SessionRewindConfig'));
});
data.gtmOnSuccess();


